.container {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 100%;
  background-color: white;
  padding: 10rpx 0 10rpx 0;
}
.inner-container {
  width: 730rpx;
  border: 1px dashed #DCEBE6;
}
.grid-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 200rpx;

}
.grid-item .img {
  width: 60rpx;
  height: 60rpx;
}
.text {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #333333;
}