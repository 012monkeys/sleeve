  <swiper indicator-dots class="swiper-img" indicator-active-color="#157658"  circular>
    <block wx:for="{{bannerB.items}}" wx:key="id">
      <swiper-item>
        <image class="swiper-img" src="{{item.img}}"></image>
      </swiper-item>
    </block>
