  <s-categrid grid="{{grid}}"></s-categrid>
  <image class="activity" src="{{activityD.entrance_img}}"></image>
  <s-spu-scroll wx:if="{{themeE.online}}" theme="{{themeE}}" spu-List="{{themeESpu}}"></s-spu-scroll>
</view>
